{
  "name": "sc-next-electron",
  "displayName": "SC - Electron and Next",
  "version": "11.0.1",
  "description": "SC - Electron and Next",
  "private": false,
  "license": "MIT",
  "main": "dist/backend/index.js",
  "author": {
    "name": "Saulo Costa",
    "email": "saulotarsobc@gmail.com",
    "url": "https://github.com/saulotarsobc"
  },
  "scripts": {
    "dev": "yarn build:backend && electron . --dev",
    "prebuild": "rimraf out && rimraf dist",
    "build:frontend": "next build src",
    "build:backend": "tsc -p backend",
    "build": "yarn build:frontend && yarn build:backend",
    "postinstall": "electron-builder install-app-deps",
    "dist": "yarn build && electron-builder",
    "update:readme": "tsx scripts/update-readme.js"
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "electron-log": "^5.4.0",
    "sc-prepare-next": "^1.0.3",
    "sequelize": "^6.37.7",
    "@libsql/client": "^0.14.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.41.0",
    "electron-serve": "^2.1.1",
    "lucide-react": "^0.513.0",
    "next": "^15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-loading-skeleton": "^3.5.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.24.2",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.8",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/xml2js": "^0.4.14",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "libxmljs": "^1.0.11",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tw-animate-css": "^1.3.4",
    "typescript-eslint": "^8.27.0",
    "xml2js": "^0.6.2"
  },
  "build": {
    "appId": "br.com.electron.next",
    "productName": "SC - Electron and Next",
    "files": [
      "dist"
    ],
    "directories": {
      "output": "out"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "${name}-${version}-windows-${arch}.${ext}"
    },
    "mac": {
      "target": "dmg",
      "signIgnore": null,
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "artifactName": "${name}-${version}-linux-${arch}.${ext}"
    }
  }
}